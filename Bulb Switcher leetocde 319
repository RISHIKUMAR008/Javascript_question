function bulbSwitch(n) {
    // The number of bulbs that remain on is the count of perfect squares <= n
    return Math.floor(Math.sqrt(n));
}

// Example usage:
console.log(bulbSwitch(3)); // Output: 1
console.log(bulbSwitch(0)); // Output: 0
console.log(bulbSwitch(1)); // Output: 1
console.log(bulbSwitch(25)); // Output: 5 (1, 4, 9, 16, 25)
console.log(bulbSwitch(10)); // Output: 3 (1, 4, 9)
