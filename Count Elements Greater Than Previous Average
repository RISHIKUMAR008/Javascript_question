function countResponseTimeRegressions(responseTimes) {
    const n = responseTimes.length;
    if (n <= 1) return 0;

    let count = 0;
    let total = responseTimes[0];

    for (let i = 1; i < n; i++) {
        // Instead of: responseTimes[i] > total / i
        if (responseTimes[i] * i > total) {
            count++;
        }
        total += responseTimes[i];
    }

    return count;
}
